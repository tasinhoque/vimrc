{
 "terminal.integrated.env.windows": {
  "COMPOSE_DOCKER_CLI_BUILD": "0"
 },
 "files.eol": "\n",
 "explorer.confirmDelete": false,
 "javascript.updateImportsOnFileMove.enabled": "always",
 "files.enableTrash": false,
 "explorer.confirmDragAndDrop": false,
 "editor.suggestSelection": "first",
 "editor.formatOnPaste": true,
 "terminal.integrated.commandsToSkipShell": [
  "workbench.action.toggleSidebarVisibility",
  "workbench.view.explorer",
  "workbench.view.search",
  "workbench.view.scm",
  "workbench.view.debug",
  "workbench.view.extensions"
 ],
 "editor.tabCompletion": "on",
 "editor.fontLigatures": true,
 "git.confirmSync": false,
 "git.enableSmartCommit": true,
 "extensions.ignoreRecommendations": true,
 "git.ignoreMissingGitWarning": true,
 "editor.cursorBlinking": "solid",
 "editor.acceptSuggestionOnEnter": "off",
 "editor.defaultFormatter": "esbenp.prettier-vscode",
 "[yaml]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[markdown]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[python]": {
  "editor.defaultFormatter": "ms-python.python",
  "editor.tabSize": 4,
  "editor.codeActionsOnSave": {
   "source.organizeImports": true
  },
  "editor.formatOnType": true
 },
 "[sql]": {
  "editor.defaultFormatter": "mtxr.sqltools"
 },
 "[json]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[jsonc]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[javascript]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "isort.args": ["--profile", "black"],
 "vim.incsearch": true,
 "vim.useSystemClipboard": true,
 "vim.useCtrlKeys": true,
 "vim.visualModeKeyBindingsNonRecursive": [
  {
   "before": ["p"],
   "after": ["p", "g", "v", "y"]
  }
 ],
 "vim.normalModeKeyBindingsNonRecursive": [
  {
   "before": ["<C-d>"],
   "after": ["<C-d>", "z", "z"]
  },
  {
   "before": ["<C-u>"],
   "after": ["<C-u>", "z", "z"]
  }
 ],
 "vim.handleKeys": {
  "<C-b>": false,
  "<C-h>": false,
  "<C-j>": false,
  "<C-g>": false,
  "<C-w>": false,
  "<C-s>": false,
  "<C-f>": false,
  "<C-k>": false
 },
 "editor.linkedEditing": true,
 "typescript.updateImportsOnFileMove.enabled": "always",
 "git.autofetch": true,
 "workbench.editorAssociations": {
  "*.ipynb": "jupyter-notebook",
  "*.xlsx": "default"
 },
 "npm.enableRunFromFolder": true,
 "workbench.iconTheme": "material-icon-theme",
 "gitlens.codeLens.enabled": false,
 "workbench.startupEditor": "none",
 "terminal.integrated.tabs.enabled": false,
 "terminal.integrated.defaultProfile.windows": "Git Bash",
 "terminal.integrated.profiles.windows": {
  "C:\\Program Files\\Git\\bin\\bash.exe (migrated)": {
   "path": "C:\\Program Files\\Git\\bin\\bash.exe",
   "args": []
  }
 },
 "security.workspace.trust.untrustedFiles": "open",
 "explorer.openEditors.visible": 0,
 "workbench.editor.untitled.hint": "hidden",
 "markdownlint.config": {
  "MD033": {
   "allowed_elements": ["details", "summary", "br", "img"]
  }
 },
 "debug.javascript.autoAttachFilter": "disabled",
 "diffEditor.renderSideBySide": false,
 "diffEditor.ignoreTrimWhitespace": false,
 "python.linting.flake8Args": ["--max-line-length=88"],
 "workbench.editor.enablePreview": false,
 "terminal.integrated.fontSize": 18,
 "notebook.outline.showCodeCells": true,
 "markdown.preview.fontSize": 20,
 "sqltools.highlightQuery": false,
 "editor.wordWrap": "on",
 "python.formatting.provider": "black",
 "editor.bracketPairColorization.enabled": false,
 "editor.codeActionsOnSave": {
  "source.organizeImports": true
 },
 "python.terminal.executeInFileDir": true,
 "python.linting.pylintCategorySeverity.warning": "Error",
 "python.linting.pylintCategorySeverity.refactor": "Error",
